{
  "name": "glug-iiitu",
  "private": true,
  "description": "IIIT Una GNU/Linux User Group",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier --write",
      "./node_modules/.bin/eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "gatsby": "^2.21.37",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-mdx": "^1.2.14",
    "gatsby-plugin-sharp": "^2.6.10",
    "gatsby-plugin-typography": "^2.5.3",
    "gatsby-remark-images": "^3.3.9",
    "gatsby-remark-prismjs": "^3.5.3",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-remark": "^2.8.14",
    "gatsby-transformer-sharp": "^2.5.4",
    "mdx-utils": "^0.2.0",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.20.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-live": "^2.2.2",
    "react-typography": "^0.16.19",
    "typescript": "^3.9.3",
    "typography": "^0.16.19",
    "typography-theme-funston": "^0.16.19"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/typography": "^0.16.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
